[kdcdefaults]
  kdc_ports = 750,88

[realms]
{{ krb5_realm|upper }} = {
  database_name = {{ krb5_kdc_database }}
  admin_keytab = FILE:{{ krb5_kdc_state_directory }}/kadm5.keytab
  acl_file = {{ krb5_kdc_state_directory }}/kadm5.acl
  key_stash_file = {{ krb5_kdc_state_directory }}/stash
  kdc_ports = 750,88
  max_life = 10h 0m 0s
  max_renewable_life = 7d 0h 0m 0s
  master_key_type = aes256-cts
  supported_enctypes = {{ krb5_enc_types }}
  default_principal_flags = +preauth
}
